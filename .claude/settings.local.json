          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "FILE=$(jq -r '.tool_input.file_path // \"\"'); if [[ \"$FILE\" == *.php ]]; then cd /Users/gonza/code/airbnb-tool && vendor/bin/pint \"$FILE\" --quiet 2>/dev/null; elif [[ \"$FILE\" =~ \\.(vue|ts|js)$ ]]; then cd /Users/gonza/code/airbnb-tool && npx eslint \"$FILE\" --fix --quiet 2>/dev/null; fi; exit 0"
          }
        ]
      }
    ]
